// Questão 2: Classes dos pacotes "a" e "b"

// Pacote: pacoteA
package pacoteA;

public class ClasseA {
    public void metodoPublico() {
        System.out.println("Método acessado!");
    }
}

// Pacote: pacoteB
package scr;

import pacoteA.ClasseA;

public class ClasseB {
    public static void main(String[] args) {
        ClasseA obj = new ClasseA();
        obj.metodoPublico();
    }
}